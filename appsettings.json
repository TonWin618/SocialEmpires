    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DbConnectionString": "<YOUR_DATABASE_CONNECTION_STRING>",
  "AzureEmailSender": {
